<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Usuario" table="usuario">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="clave" type="string" column="clave" length="50" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="nombreUsuario" type="string" column="nombre_usuario" length="50" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="nombre" type="string" column="nombre" length="500" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="apellido" type="string" column="apellido" length="500" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="status" type="string" column="status" length="100" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="fechaCreacion" type="string" column="fecha_creacion" length="100" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="fechaUltimoIngreso" type="string" column="fecha_ultimo_ingreso" length="100" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="tipo" type="string" column="tipo" length="100" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="comentario" type="string" column="comentario" length="500" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>

<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Cotizaciones" table="cotizaciones">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="cliente" type="string" column="cliente" length="500" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="obra" type="string" column="obra" length="500" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="fecha" type="string" column="fecha" length="100" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="devolucion" type="string" column="devolucion" length="100" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="status" type="string" column="status" length="100" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="cantidad" type="integer" column="cantidad" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="clave" type="string" column="clave" length="100" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="equipo" type="string" column="equipo" length="100" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="dias" type="integer" column="dias" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="pu" type="float" column="PU" precision="10" scale="0" nullable="false"/>
    <field name="importe" type="float" column="importe" precision="10" scale="0" nullable="false"/>
    <field name="direccionEntrega" type="string" column="direccion_entrega" length="500" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="comentarios" type="string" column="comentarios" length="500" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="descuento" type="float" column="descuento" precision="10" scale="0" nullable="false"/>
    <field name="impuesto" type="float" column="impuesto" precision="10" scale="0" nullable="false"/>
    <field name="total" type="float" column="total" precision="10" scale="0" nullable="false"/>
    <field name="subtotal" type="float" column="subtotal" precision="10" scale="0" nullable="false"/>
    <field name="subtotal2" type="float" column="subtotal2" precision="10" scale="0" nullable="false"/>
    <field name="servicioentrega" type="float" column="servicioentrega" precision="10" scale="0" nullable="false"/>
    <field name="fechahoy" type="string" column="fechaHoy" length="100" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="cotizacion" type="integer" column="cotizacion" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="pedido" type="integer" column="pedido" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
